cf apps //Show all apps available in CF

cf delete <app-name> //To delete the app from CF

cf push <app-name> //To push app into CF
cf push <app-name> route//To push app into CF

cf push <app-name> -p <app-name>.jar --random-route //random route appending app with the cloud url or end-point

cf push pal-tracker -i 2 -p build/libs/pal-tracker.jar --random-route //increase the no of instance i 2

cf logs <app-name> --recent //To see recent logs while pushing apps

cf set-env <app-name> JBP_CONFIG_OPEN_JDK_JRE '{ jre: { version: 11.+ } }' //To set jre in CF Env

cf set-env pal-tracker ENV_VAR_VALUE '{WELCOME_MESSAGE:"Hello Command"}'
cf stop <app-name> //to stop app that running on CF
cf help set-env
cf env <app-name>

//**below command used to bind database service in cloud through Flyway
cf marketplace // get all service list
cf marketplace //command to find a MySQL database service on your Pivotal Cloud Foundry installation
cf create-service // command to create an instance of this service named tracker-database in the review space
cf create-service clearDB <plan-name> <servicename>
cf create-service clearDB spark tracker-database
cf s //Show all the service list
cf bind-service pal-tracker tracker-database //It is used to bind service db to app in cloud
cf service ${SERVICE_NAME} //Confirm that the service instance is being created
cf service tracker-database
